<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             Title="Terms of Use"
             x:Class="GridCentral.Views.Settings.TermsOfService">
    <ContentPage.Content>
        <StackLayout>
            <Label Text="Terms of Use" FontSize="Large" FontAttributes="Bold" Margin="10"/>
            <Frame Margin="10">
                <Label x:Name="Termslbl" FontSize="Small"/>
            </Frame>
        </StackLayout>
    </ContentPage.Content>
</ContentPage>